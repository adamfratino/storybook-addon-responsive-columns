{
  "name": "storybook-addon-responsive-columns",
  "version": "2.0.0",
  "description": "A responsive, grid-based columns overlay.",
  "author": "Adam Fratino <hello@fratino.dev>",
  "keywords": [
    "columns",
    "grid",
    "guides",
    "layout",
    "design",
    "addon",
    "storybook",
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adamfratino/storybook-addon-responsive-columns"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "buildBabel": "concurrently \"yarn buildBabel:cjs\" \"yarn buildBabel:esm\"",
    "buildBabel:cjs": "babel ./src -d ./dist/cjs --extensions \".js,.jsx,.ts,.tsx\"",
    "buildBabel:esm": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "prebuild": "yarn clean",
    "build": "concurrently \"yarn buildBabel\" \"yarn buildTsc\"",
    "build:watch": "concurrently \"yarn buildBabel:esm -- --watch\" \"yarn buildTsc -- --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "start": "concurrently \"yarn build:watch\" \"yarn storybook -- --no-manager-cache --quiet\"",
    "build-storybook": "storybook build",
    "prerelease": "zx scripts/prepublish-checks.mjs",
    "release": "yarn build && auto shipit",
    "eject-ts": "zx scripts/eject-typescript.mjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.13.0",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/components": "^7.5.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/preview-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "auto": "^10.3.0",
    "babel-loader": "^8.1.0",
    "boxen": "^5.0.1",
    "concurrently": "^6.2.0",
    "dedent": "^0.7.0",
    "prettier": "^2.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "storybook": "^7.5.3",
    "typescript": "^4.2.4",
    "zx": "^1.14.1"
  },
  "peerDependencies": {
    "@storybook/api": "^7.5.3",
    "@storybook/components": "^7.5.3",
    "@storybook/core-events": "^7.5.3",
    "@storybook/theming": "^7.5.3",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Responsive Columns",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ],
    "icon": "https://raw.githubusercontent.com/adamfratino/storybook-addon-responsive-columns/main/icon.png"
  }
}
